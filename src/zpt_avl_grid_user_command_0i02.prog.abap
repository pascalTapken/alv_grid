MODULE user_command_0100 INPUT.
  CASE okcode.
    WHEN 'BACK' OR 'EXIT' OR 'CANC'.
      SET SCREEN 0.
      LEAVE SCREEN.
    WHEN OTHERS.
      clear okcode.
  ENDCASE.
ENDMODULE.
